{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    }
  },
  "globalEnv": [
    "PROTOCOL_ENV",
    "NODE_ENV",
    "PROTOCOL_URL",
    "VERCEL_URL",
    "API_URL",
    "RENDER_INTERNAL_HOSTNAME",
    "PORT"
  ]
}
